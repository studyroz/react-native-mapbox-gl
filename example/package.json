    "@turf/line-distance": "^4.7.3",
    "@turf/nearest": "^4.7.3",
    "buffer": "^5.1.0",
    "install": "^0.12.2",
    "moment": "^2.24.0",
    "npm": "^6.13.4",
    "prop-types": "^15.7.2",
    "react": "16.9.0",
